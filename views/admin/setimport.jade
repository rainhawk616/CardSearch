extends layout

block content
    h1= title
    p Upload sets information in JSON format
    form(action="/admin/set/import" method="POST" enctype="multipart/form-data")
        .form-group
            input#setsimport(type="file" name="setsimport" required)
        .form-group.btn-toolbar
            button.btn.btn-primary(type="submit") Upload
            a.btn.btn-default(onclick="back()") Back